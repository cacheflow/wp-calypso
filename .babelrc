{
	"presets": [
		[ "env", {
			"targets": {
				"browsers": [
					"last 2 versions",
					"Safari >= 10",
					"iOS >= 10",
					"not ie <= 10"
				]
			},
			"modules": false
		} ],
		"stage-2"
	],
	"plugins": [
		"add-module-exports",
		"syntax-jsx",
		["transform-class-properties", { "spec": true } ],
		"transform-export-extensions",
		"transform-react-display-name",
		"transform-react-jsx",
		"transform-runtime",
		[
			"transform-imports",
			{
				"state/selectors": {
					"transform": "state/selectors/${member}",
					"kebabCase": true
				}
			}
		]
	],
	"env": {
		"test": {
			"plugins": [
				[ "transform-builtin-extend", {
					"globals": [ "Error" ]
				} ],
			]
		}
	}
}
